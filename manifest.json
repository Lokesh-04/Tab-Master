{
    "manifest_version": 3,
    "name": "TabMaster",
    "version": "1.0",
    "description": "Manage your tabs efficiently.",
    "action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "tabs"
    ],
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  